entity Article {
    designation String,
    stock Integer,
    nbPoints Integer,
    description String,

}

entity Categorie {
    countryName String
}




relationship OneToMany {
    Article to Categorie{categorie},
    Commande to Article{article}
}

relationship ManyToOne {
    Categorie{categorie} to Article,
    Article{article} to Commande
}
